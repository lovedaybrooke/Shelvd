#!/Users/katmatfield/Dropbox/Python/Trial/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'twitter==1.10.0','console_scripts','twitter-log'
__requires__ = 'twitter==1.10.0'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('twitter==1.10.0', 'console_scripts', 'twitter-log')()
    )
